{
  "$schema": "https://mintlify.com/schema.json",
  "name": "pvp.trade",
  "logo": {
    "dark": "https://pvp.trade/icon.svg",
    "light": "https://pvp.trade/icon.svg"
  },
  "theme": "quill",
  "search": {
    "location": "top"
  },
  "modeToggle": {
    "default": "dark",
    "isHidden": true
  },
  "background": {
    "style": "grid"
  },
  "favicon": "/images/icon.svg",
  "colors": {
    "primary": "#ffffff",
    "light": "#ffffff",
    "dark": "#ffffff",
    "anchors": {
      "from": "#ffffff",
      "to": "#ffffff"
    }
  },
  "tabs": [],
  "anchors": [],
  "navigation": [
    {
      "group": "Documentation",
      "pages":  ["en-quickstart", "en-commands", "en-points", "en-faq"],
      "version": "english"
    }
    {
      "group": "Documentation",
      "pages":  ["cn-quickstart", "cn-commands", "cn-points", "cn-faq"],
      "version": "chinese"
    }
  ],
  "footerSocials": {
    "x": "https://x.com/pvp_dot_trade"
  },
  "versions": [
    {"name":"english",
      "default": true,
      "locale": "en"},
    {"name":"chinese",
    "locale":"cn"}]
}